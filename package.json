{
  "name": "uixtra",
  "version": "1.0.6",
  "description": "UI Components, Utility Functions",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && rollup -c --bundleConfigAsCjs",
    "postbuild": "node ./scripts/postBuild.js"
  },
  "repository": "git@github.com:luciferRK/uixtra.git",
  "author": "lucifer_rk",
  "license": "MIT",
  "private": false,
  "exports": {
    "./components": {
      "types": "./components/index.d.ts",
      "import": "./components/index.esm.js",
      "require": "./components/index.cjs.js"
    },
    "./utils": {
      "types": "./utils/index.d.ts",
      "import": "./utils/index.esm.js",
      "require": "./utils/index.cjs.js"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "postcss": "^8.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "6.0.0",
    "rollup": "^4.34.4",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.84.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "tslib": "2.6.3"
  },
  "peerDependencies": {
    "react": ">=17.0.2 <20.0.0",
    "react-dom": ">=17.0.2 <20.0.0"
  }
}
